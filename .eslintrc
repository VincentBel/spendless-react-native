{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": false,
    "node": true
  },
  "rules": {
    "global-require": 0,
    "new-cap": 0,
    "no-nested-ternary": 0,
    "no-underscore-dangle": 0,
    "semi": [2, "never"]
  },
  "globals": {
    "__DEV__": true,
    "fetch": false,
    "window": false,
    "require": false
  }
}
